          },
          "id": {
            "type": "string",
            "description": "User's id",
            "example": "_id : 63e380c194de20f1761df4ae"
          },
          "email": {
            "type": "string",
            "description": "User's email",
            "format": "email"
          }
        }
      },
      "logoutResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Success"
          }
        }
      },
      "UpdateRequest": {
        "type": "object",
        "properties": {
          "avatarURL": {
            "type": "string",
            "format": "binary",
